	<option onclick="popperup(400,500,'<%= request.getContextPath() %>/oscarEncounter/calculators/SimpleCalculator.jsp','SimpleCalc');">
		<fmt:setBundle basename="oscarResources"/><fmt:message key="oscarEncounter.Index.simpleCalculator"/>
	</option>

	<option onclick="popperup(650,775,'<%= request.getContextPath() %>/oscarEncounter/calculators/GeneralCalculators.jsp','GeneralConversions'); ">
		<fmt:setBundle basename="oscarResources"/><fmt:message key="oscarEncounter.Index.generalConversions"/>
	</option>
</select>