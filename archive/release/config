echo  ${C_HOME}${PROGRAM}.properties created >>$LOG_FILE
echo "the oscar properties file contains a plaintext password..." >>$LOG_FILE
echo "so let only tomcat read it" >>$LOG_FILE
chown ${TOMCAT}:${TOMCAT} ${C_HOME}${PROGRAM}.properties 2>>$LOG_FILE
chmod 640 ${C_HOME}${PROGRAM}.properties 2>>$LOG_FILE

# some people look for /var/lib/tomcat8/drugref2.properties
# so create a symlink
ln -s /usr/share/${TOMCAT}/${PROGRAM}.properties /var/lib/${TOMCAT}/${PROGRAM}.properties

echo "configure crontab" >>$LOG_FILE

chmod 711 ${SRC}oscar_backup.sh  2>>$LOG_FILE
chmod 711 ${SRC}reOscar.sh  2>>$LOG_FILE
chmod 711 ${SRC}gateway.cron  2>>$LOG_FILE

if [ -f /var/spool/cron/crontabs/root ] ; then
	# "checking existing crontab" >>$LOG_FILE
	echo "write out current crontab" >>$LOG_FILE
	crontab -l > c.temp 2>>$LOG_FILE
else
	touch c.temp 2>>$LOG_FILE
fi

if grep -Fq "01 23 * * * ${SRC}oscar_backup.sh" c.temp ; then
    	echo "entry for oscar_backup already present" >>$LOG_FILE
else
	echo "append the backup job to a new cron file" >>$LOG_FILE
    	echo "01 23 * * * ${SRC}oscar_backup.sh" >> c.temp 2>>$LOG_FILE
fi 
if grep -Fq "* * * * * ${SRC}reOscar.sh" c.temp ; then
    	echo "entry for reOscar already present" >>$LOG_FILE
else
	echo "append the reOscar job to a new cron file" >>$LOG_FILE
    	echo "* * * * * ${SRC}reOscar.sh" >> c.temp 2>>$LOG_FILE
fi 
if grep -Fq "*/3 * * * * ${SRC}gateway.cron" c.temp ; then
    	echo "entry for gateway fax already present" >>$LOG_FILE
else
	echo "append the gateway fax job to a new cron file" >>$LOG_FILE
    	echo "* * * * * ${SRC}gateway.cron" >> c.temp 2>>$LOG_FILE
fi 
if grep -Fq "00 23 * * Fri touch /tmp/${TOMCAT}-${TOMCAT}-tmp/restartOscar.action" c.temp ; then
    	echo "entry for restart OSCAR weekly already present" >>$LOG_FILE
else
	echo "append restart OSCAR weekly job to a new cron file" >>$LOG_FILE
    	echo "00 23 * * Fri touch /tmp/${TOMCAT}-${TOMCAT}-tmp/restartOscar.action" >> c.temp 2>>$LOG_FILE
fi


#	echo "Commenting out previous oscar backup jobs" >>$LOG_FILE
#	sed -e 's/\(^[^#].*oscar_backup\)/#\1/' c.temp > tempcron 2>>$LOG_FILE


echo "install new cron file" >>$LOG_FILE
crontab c.temp 2>>$LOG_FILE
rm c.temp 2>>$LOG_FILE




