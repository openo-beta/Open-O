# Only cleanup the passwords on error because we may need the passwords
# in the post install script. The post install must handle the cleanup
# for all error conditions and successful exit.
trap cleanupOnError HUP TERM ABRT KILL INT QUIT 
cleanupOnError()
{
	echo "Cleaning up sensitive data"
	db_get oscar-emr/password
}

# --- log the running of the script appending as necessary
echo "#########" `date` "#########" 1>> $LOG_FILE
echo Configuration of VERSION=${VERSION}-${REVISION}>>$LOG_FILE


# dpkg -l |grep " tomcat"
#/usr/share/tomcat7/bin/version.sh

# log versions for troubleshooting
/usr/share/${TOMCAT}/bin/version.sh >> $LOG_FILE
mysql -V  >> $LOG_FILE
echo "" >>$LOG_FILE

invoke-rc.d ${TOMCAT} stop 2>>$LOG_FILE

if [ -f ${C_BASE}/${PACKAGE}/WEB-INF/classes/clientKeystore.properties ] ; then
        echo "retrieve existing clientKeystore.properties" >> $LOG_FILE
        cp ${C_BASE}/${PACKAGE}/WEB-INF/classes/clientKeystore.properties ${C_BASE}/${PACKAGE}/ck.properties 1>> $LOG_FILE
fi

# if tomcat8 then need to move the wars from the DEB installed directories to the tomcat8 directory
if [ -f /usr/share/tomcat8/bin/version.sh ] ; then
        mv /var/lib/tomcat7/webapps/oscar.war /var/lib/tomcat8/webapps/oscar.war
        mv /var/lib/tomcat7/webapps/drugref.war /var/lib/tomcat8/webapps/drugref.war
fi

#GEOIP=$(curl --silent -f http://freegeoip.net/csv/ |  tr -d '"')
#echo GEOIP=$GEOIP >>$LOG_FILE
#IP=$(echo $GEOIP | awk -F "," '{print $1;}')
#COUNTRY=$(echo $GEOIP | awk -F "," '{print $2;}')
#STATE=$(echo $GEOIP | awk -F "," '{print $4;}')
#LOCALITY=$(echo $GEOIP | awk -F "," '{print $6;}')

GEOIP2=$(curl --silent -f http://ip-api.com/csv/ |  tr -d '"' | tr ' ' '+')
echo GEOIP2=$GEOIP2 >>$LOG_FILE
IP=$(echo $GEOIP2 | awk -F "," '{print $14;}')
COUNTRY=$(echo $GEOIP2 | awk -F "," '{print $3;}')
STATE=$(echo $GEOIP2 | awk -F "," '{print $4;}')
LOCALITY=$(echo $GEOIP2 | awk -F "," '{print $6;}')
echo State=${STATE} >>$LOG_FILE

#if [ "${STATE}" = "QC" ]; then
#	util_erx_clinic_locale="1"
#fi
#if [ "${COUNTRY}" = "US" ]; then
#	util_erx_enabled=true
#	util_erx_clinic_locale="3"
#fi


#echo "If upgrade possible, ask about upgrade" >>$LOG_FILE
#if [ -f /usr/share/tomcat7/oscar.properties ] ; then
#	echo "Previous Oscar 15 Installation Detected" >> $LOG_FILE
#    	db_input high oscar-emr/upgrade || true
#	db_go ||true
#	db_get oscar-emr/upgrade
#	UPGRADE=$RET
#	echo "UPGRADE=${UPGRADE}" 1>> $LOG_FILE
#fi

echo "Checking for Upgrade possibility" >>$LOG_FILE
if [ -f /usr/share/${TOMCAT}/oscar.properties ] ; then
	echo "Previous OSCAR Installation Detected" >> $LOG_FILE
	UPGRADE=true
	echo "UPGRADE=${UPGRADE}" 1>> $LOG_FILE
fi


if [ "${UPGRADE}" = "false" ] ; then
echo "For a new install from console so probe for some settings" >>$LOG_FILE
	db_set oscar-emr/upgrade false
	db_input high oscar-emr/password || true
	db_go || true
	db_get oscar-emr/password
	db_password=$RET

	#echo ${db_password} >>$LOG_FILE
	db_reset oscar-emr/billregion || true
	# use probed state and configure for BC if required
	if [ "${STATE}" = "BC" ]; then
		billregion="BC"
	fi
	echo "billregion=${billregion}" 1>> $LOG_FILE
	db_set oscar-emr/billregion $billregion || true
	db_reset oscar-emr/phoneprefix || true
	# probe for local area code
	phoneprefix=$(curl --silent -f https://www.zip-codes.com/canadian/city.asp?city=$LOCALITY\&province=$STATE |  grep '<a href="/canadian/area-code.asp?areacode='| sed "s/.*areacode=\([0-9]*\).*/\1/;s/^[[:space:]]*//;s/[[:space:]]*$//" )
## curl --silent -f 'https://www.zip-codes.com/canadian/city.asp?city=Timmins&province=ON' |  grep '<a href="/canadian/area-code.asp?areacode='| sed "s/.*areacode=\([0-9]*\).*/\1/;s/^[[:space:]]*//;s/[[:space:]]*$//"
	echo "area code aka phone prefix=*${phoneprefix}*" 1>> $LOG_FILE

	# the area code may hint the billingcenter
	if [ "$phoneprefix" = "705" ]; then
		billcenter="R"
		default_bill_center="R"
		echo "billcenter set to R Sudbury" 1>> $LOG_FILE
	fi
	if [ "${phoneprefix}" = "807" ]; then
		billcenter="U"
		default_bill_center="U"
		echo "billcenter set to U Thunder Bay" 1>> $LOG_FILE
	fi
	if [ "${phoneprefix}" = "416" ]; then
		billcenter="N" 
		default_bill_center="N"
		echo "billcenter set to N Toronto" 1>> $LOG_FILE
	fi
	if [ "${phoneprefix}" = "613" ]; then
		billcenter="D" 
		default_bill_center="D"
		echo "billcenter set to D Ottawa but could be J Kingston" 1>> $LOG_FILE
	fi
	echo "Setting initial defaults for areacode ${phoneprefix} in ${STATE}"
	db_reset oscar-emr/triumvirate || true
	db_set oscar-emr/triumvirate  true  || true
		
	db_input high oscar-emr/demo || true

	db_reset oscar-emr/eyeform || true
	db_set oscar-emr/eyeform false || true
	eyeform=false
	db_set oscar-emr/appt_intake_form true || true
	appt_intake_form="on"

	db_go || true

	db_get oscar-emr/eyeform
	eyeform=$RET
	echo "eyeform=${eyeform}" 1>> $LOG_FILE
fi




if [ "${triumvirate}" = "true" ] ; then
	echo "set properties for signatures, fax and pdf printing" >>$LOG_FILE
	rx_enhance=false
	workflow_enhance=true

	# Consult: fax and signature capability for consultation requests requires flags:
	# faxEnable=yes,consultation_fax_enabled=true,consultation_signature_enabled=true
	enableFax=true
	faxEnable=yes
	consultation_fax_enabled=true
	consultation_signature_enabled=true
	consultation_dynamic_labelling_enabled=true
	consultation_indivica_attachment_enabled=true

	# Rx: fax functionality For prescript requires flags:
	#rx_signature_enabled=true,rx_fax_enabled=true
	rx_signature_enabled=true
	rx_fax_enabled=true

	# eForm: 
	eform_signature_enabled=true
	indivica_rich_text_letter_enabled=true
	eform_fax_enabled=true
	eform_generator_indivica_signature_enabled=true
	eform_generator_indivica_print_enabled=true
	eform_generator_indivica_fax_enabled=true

fi

if [ "${UPGRADE}" = "true" ] ; then
	db_set oscar-emr/upgrade true  || true

	echo "check to see if minor upgrade of war is all that is necessary" >>$LOG_FILE
	if [ -f ${C_HOME}${PROGRAM}.properties ] ; then

		echo "grep the password from the properties file" >> $LOG_FILE
		db_password=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'db_password'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
		echo "grep the previous db_name from the properties file" >> $LOG_FILE
		## you want oscar_15=oscar_15 but trim off the switches that we might be sending
		existing=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'db_name'  | tail -n 1 | cut -d "=" -f2 | cut -d "?" -f1 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
		if [ "${db_name}" = "${existing}" ] ; then

            # UPDATE late breaking property key values here
	        echo "adding new properties file key values"  >>$LOG_FILE

		    CONSULTATION_APPOINTMENT_INSTRUCTIONS_LOOKUP=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'CONSULTATION_APPOINTMENT_INSTRUCTIONS_LOOKUP'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${CONSULTATION_APPOINTMENT_INSTRUCTIONS_LOOKUP}" = "" ]; then
                echo "adding consult key value block"  >>$LOG_FILE

                echo "# True to enable the use of the customizable Appointment Instructions look up list" >> ${C_HOME}${PROGRAM}.properties 
                echo "# in the consultation module.  It is suggested to toggle CONSULTATION_PATIENT_WILL_BOOK" >> ${C_HOME}${PROGRAM}.properties 
                echo "# to false." >> ${C_HOME}${PROGRAM}.properties 
                echo "CONSULTATION_APPOINTMENT_INSTRUCTIONS_LOOKUP=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "# True to enable the Patient will book checkbox in the consultation request form." >> ${C_HOME}${PROGRAM}.properties 
                echo "CONSULTATION_PATIENT_WILL_BOOK=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "# False to allow changes to the consultation referral date. Otherwise it will be" >> ${C_HOME}${PROGRAM}.properties 
                echo "# locked to the date the referral was created." >> ${C_HOME}${PROGRAM}.properties 
                echo "CONSULTATION_LOCK_REFERRAL_DATE=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "# Set to true to allow all active patient allergies to auto-populate into a " >> ${C_HOME}${PROGRAM}.properties 
                echo "# consultation request when opened." >> ${C_HOME}${PROGRAM}.properties 
                echo "# buttons are available in the form to manually populate Medications" >> ${C_HOME}${PROGRAM}.properties 
                echo "CONSULTATION_AUTO_INCLUDE_ALLERGIES=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "# Set to true to allow all active patient Medications to auto-populate" >> ${C_HOME}${PROGRAM}.properties 
                echo "# into a consultation request when opened." >> ${C_HOME}${PROGRAM}.properties 
                echo "# buttons are available in the form to manually populate Medications" >> ${C_HOME}${PROGRAM}.properties 
                echo "CONSULTATION_AUTO_INCLUDE_MEDICATIONS=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi

            BILLING_REVIEW_AUTO_PAYMENT=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'BILLING_REVIEW_AUTO_PAYMENT'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${BILLING_REVIEW_AUTO_PAYMENT}" = "" ]; then
                echo "adding billing and invoice block"  >>$LOG_FILE 
                echo "#Enable auto populating of payment field on billing review" >> ${C_HOME}${PROGRAM}.properties 
                echo "BILLING_REVIEW_AUTO_PAYMENT=no" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#Private billing letterhead logo image document type" >> ${C_HOME}${PROGRAM}.properties 
                echo "invoice_head_logo_doctype=invoice letterhead" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi
            new_label_print=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'new_label_print'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${new_label_print}" = "" ]; then
                echo "adding new label print key"  >>$LOG_FILE
                echo "#set to true to enable" >> ${C_HOME}${PROGRAM}.properties 
                echo "new_label_print = false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi

            tickler_default_assigned_provider=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'tickler_default_assigned_provider'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${tickler_default_assigned_provider}" = "" ]; then
                echo "adding new tickler assignment default key"  >>$LOG_FILE 
                echo "# Default ticklers to the assigned provider" >> ${C_HOME}${PROGRAM}.properties 
                echo "# True: Set the default report for Ticklers main interface to be set to the current provider" >> ${C_HOME}${PROGRAM}.properties 
                echo "# False: Ticklers main interface always default to report ALL providers" >> ${C_HOME}${PROGRAM}.properties 
                echo "tickler_default_assigned_provider=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
             fi

            DEMOGRAPHIC_PATIENT_HEALTH_CARE_TEAM=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'DEMOGRAPHIC_PATIENT_HEALTH_CARE_TEAM'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${DEMOGRAPHIC_PATIENT_HEALTH_CARE_TEAM}" = "" ]; then
                echo "adding DEMOGRAPHIC options block"  >>$LOG_FILE 
                echo "DEMOGRAPHIC_PATIENT_HEALTH_CARE_TEAM=true"  >> ${C_HOME}${PROGRAM}.properties 
                echo "DEMOGRAPHIC_PATIENT_CLINIC_STATUS=true"  >> ${C_HOME}${PROGRAM}.properties 
                echo "DEMOGRAPHIC_PATIENT_ROSTERING=true"  >> ${C_HOME}${PROGRAM}.properties 
                echo "DEMOGRAPHIC_WAITING_LIST=true"  >> ${C_HOME}${PROGRAM}.properties
                echo "" >> ${C_HOME}${PROGRAM}.properties
            fi

            referral_menu=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'referral_menu'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${referral_menu}" = "" ]; then
                echo "adding referral menue options block"  >>$LOG_FILE 
                echo "#hide the drug of choice button from rx3 interface"  >> ${C_HOME}${PROGRAM}.properties
                echo "#rx.drugofchoice.hide=true"  >> ${C_HOME}${PROGRAM}.properties
                echo ""  >> ${C_HOME}${PROGRAM}.properties
                echo "#ONTARIO_MD_INCOMINGREQUESTOR=yes"  >> ${C_HOME}${PROGRAM}.properties
                echo ""  >> ${C_HOME}${PROGRAM}.properties 
                echo "#show a menu in the main bar on schedule page to search professional specialists"  >> ${C_HOME}${PROGRAM}.properties 
                echo "referral_menu=no"  >> ${C_HOME}${PROGRAM}.properties 
                echo ""  >> ${C_HOME}${PROGRAM}.properties 
                echo "#by default, when using the caisi registration intake, saving it causes the provider to become the MRP."  >> ${C_HOME}${PROGRAM}.properties 
                echo "#set this to false to avoid that from happening."  >> ${C_HOME}${PROGRAM}.properties 
                echo "caisi.registration_intake.updateMRPOnSave=true"  >> ${C_HOME}${PROGRAM}.properties
                echo "" >> ${C_HOME}${PROGRAM}.properties
            fi

            born_orgcode=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'born_orgcode'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${born_orgcode}" = "" ]; then
                echo "adding kidconnect block"  >>$LOG_FILE 
                echo "#for kidconnect 0-5 program"  >> ${C_HOME}${PROGRAM}.properties
                echo "#unique identifier provided by BORN"  >> ${C_HOME}${PROGRAM}.properties
                echo "born_orgcode=mcmaster"  >> ${C_HOME}${PROGRAM}.properties
                echo "#names of all the forms used in the project"  >> ${C_HOME}${PROGRAM}.properties
                echo "born_eform_rourke=Rourke Baby Record"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_sumrptmarkers=Summary Report: Well Baby Visit"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds1m2m=NDDS 1&2 Months"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds4m=NDDS 4 Months"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds6m=NDDS 6 Months"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds9m=NDDS 9 Months"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds12m=NDDS 12 Months"  >> ${C_HOME}${PROGRAM}.properties
                echo "born_eform_ndds15m=NDDS 15 Months" >> ${C_HOME}${PROGRAM}.properties
                echo "born_eform_ndds18m=NDDS 18 Months"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds24m=NDDS 24 Months"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds30m=NDDS 30 Months"  >> ${C_HOME}${PROGRAM}.properties 
                echo "born_eform_ndds3y=NDDS 3 Years"  >> ${C_HOME}${PROGRAM}.properties
                echo "born_eform_ndds4y=NDDS 4 Years" >> ${C_HOME}${PROGRAM}.properties
                echo "born_eform_ndds5y=NDDS 5 Years"  >> ${C_HOME}${PROGRAM}.properties
                echo "born_eform_ndds6y=NDDS 6 Years" >> ${C_HOME}${PROGRAM}.properties
                echo ""  >> ${C_HOME}${PROGRAM}.properties
            fi

            ENCOUNTER_TIME_MANDATORY=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'ENCOUNTER_TIME_MANDATORY'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${ENCOUNTER_TIME_MANDATORY}" = "" ]; then
                echo "adding end block"  >>$LOG_FILE
                echo "ENCOUNTER_TIME_MANDATORY=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "TRANSPORTATION_TIME_MANDATORY=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#this will make patient search have a wildcard at front of it" >> ${C_HOME}${PROGRAM}.properties 
                echo "search.searchName.addLeadingWildcard=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "## A new patient consent module in a nice versatile and compact package. " >> ${C_HOME}${PROGRAM}.properties 
                echo "## View instruction documentation." >> ${C_HOME}${PROGRAM}.properties 
                echo "USE_NEW_PATIENT_CONSENT_MODULE=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#in the master demo" >> ${C_HOME}${PROGRAM}.properties 
                echo "skip_postal_code_validation=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#don't include MRP in print report by setting to false" >> ${C_HOME}${PROGRAM}.properties 
                echo "print.includeMRP=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo " #if true, it will use the provider's current program instead of clinic contact info" >> ${C_HOME}${PROGRAM}.properties 
                echo "print.useCurrentProgramInfoInHeader=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#Turn on resident review workflow" >> ${C_HOME}${PROGRAM}.properties 
                echo "#resident_review=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "#custom methodone and suboxone" >> ${C_HOME}${PROGRAM}.properties 
                echo "enable_rx_custom_methodone_suboxone=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "#add watermark in rx prescription" >> ${C_HOME}${PROGRAM}.properties 
                echo "enable_rx_watermark=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "#Rx watermark png file name is in oscar images folder" >> ${C_HOME}${PROGRAM}.properties 
                echo "# If no this file name, the default one would be oscar service watermark." >> ${C_HOME}${PROGRAM}.properties 
                echo "#rx_watermark_file_name=north_toronto_eye_care_watermark.png" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#Control enable or disable prevention billing " >> ${C_HOME}${PROGRAM}.properties 
                echo "ENABLE_PREVENTION_BILLING=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#showPrimaryCarePhysicianCheck=yes" >> ${C_HOME}${PROGRAM}.properties 
                echo "#showEmploymentStatus=yes" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#do not allow user to change password to ones previously used" >> ${C_HOME}${PROGRAM}.properties 
                echo "password.pastPasswordsToNotUse=0" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi

            enable_create_child_record=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'enable_create_child_record'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${enable_create_child_record}" = "" ]; then
                echo "adding ad hoc"  >>$LOG_FILE  
                echo "enable_create_child_record=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "rx.enable_internal_dispensing=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
                echo "#show hamilton public health items in admin menu" >> ${C_HOME}${PROGRAM}.properties 
                echo "admin.hph=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties
            fi

            OnCallClinic=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'OnCallClinic'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${OnCallClinic}" = "" ]; then
                echo "adding OnCallClinic key value"  >>$LOG_FILE  
                echo "#Turn on OSCAR ON CALL Clinic interface yes|no" >> ${C_HOME}${PROGRAM}.properties 
                echo "OnCallClinic=yes" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi

            shared_outcomes_dashboard_host=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'shared_outcomes_dashboard_host'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${shared_outcomes_dashboard_host}" = "" ]; then
                echo "adding shared_outcomes_dashboard_host"  >>$LOG_FILE  
                echo "shared_outcomes_dashboard_host=true" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi
            
            # rx_strict_med_term added March 14th, 2017
            rx_strict_med_term=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'rx_strict_med_term'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${rx_strict_med_term}" = "" ]; then
                echo "adding rx_strict_med_term"  >>$LOG_FILE 
                echo "#validate medication term in rx prescription" >> ${C_HOME}${PROGRAM}.properties
                echo "rx_strict_med_term=false" >> ${C_HOME}${PROGRAM}.properties 
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi

            # on ar changes to allow for label for prenatal screening added July 4, 2017
            prenatal_screening_eform_group=$(sed '/^\#/d' ${C_HOME}${PROGRAM}.properties | grep 'prenatal_screening_eform_group'  | tail -n 1 | cut -d "=" -f2 | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
            if [ "${prenatal_screening_eform_group}" = "" ]; then
                echo "adding prenatal_screening_eform_group"  >>$LOG_FILE 
                echo "#ONAR label for IPS/FTS" >> ${C_HOME}${PROGRAM}.properties
                echo "prenatal_screening_eform_group=FTS" >> ${C_HOME}${PROGRAM}.properties 
                echo "prenatal_screening_name=First Trimester Screening" >> ${C_HOME}${PROGRAM}.properties
                echo "prenatal_screening_abbrv=FTS" >> ${C_HOME}${PROGRAM}.properties
                echo "" >> ${C_HOME}${PROGRAM}.properties 
            fi

			echo "${PROGRAM}.war file updated so exiting config normally" >> $LOG_FILE
			exit 0
		else
			echo "${PROGRAM}.war is not deb installed OSCAR_15 and cannot be deb updated so exiting config" 1>> $LOG_FILE
			exit 1		
		fi

	fi

	echo "save a copy of the previous properties file"  >>$LOG_FILE
	if [ -f /usr/share/${TOMCAT}/Oscar${PREVIOUS}.properties ] ; then
		echo Oscar${PREVIOUS}.properties found 1>> $LOG_FILE
		cp /usr/share/${TOMCAT}/Oscar${PREVIOUS}.properties ${PROGRAM}.tmp
		skip12=true
		skip121=true
	else
		if [ -f /usr/share/tomcat6/Oscar12.properties ] ; then
			cp /usr/share/tomcat6/Oscar12.properties ${PROGRAM}.tmp
			skip12=true
		else
			if [ -f /usr/share/tomcat6/Oscar11.properties ] ; then
				cp /usr/share/tomcat6/Oscar11.properties ${PROGRAM}.tmp
				PREVIOUS=11
			else
				if [ -f /usr/share/tomcat6/Oscar10_12.properties ] ; then
					cp ${C_HOME}Oscar10_12.properties ${PROGRAM}.tmp
					PREVIOUS=10_12
				else
					echo "no previous properties file found to upgrade"  1>>$LOG_FILE
					exit 1
				fi
			fi
		fi
	fi
	# drop lines that start with a comment, then grep the property, just take the last instance of that, cut on the = delimiter, and trim whitespace
	echo "grep the password from the properties file" >> $LOG_FILE
	db_password=$(sed '/^\#/d' ${PROGRAM}.tmp | grep 'db_password'  | tail -n 1 | cut -d "=" -f2- | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
	echo "grep the province from the properties file" >> $LOG_FILE
	billregion=$(sed '/^\#/d' ${PROGRAM}.tmp | grep 'billregion'  | tail -n 1 | cut -d "=" -f2- | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
	echo "billregion=${billregion}" 1>> $LOG_FILE
	echo "grep the phone prefix from the properties file" >> $LOG_FILE
	phoneprefix=$(sed '/^\#/d' ${PROGRAM}.tmp | grep 'phoneprefix'  | tail -n 1 | cut -d "=" -f2- | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
	echo "phoneprefix=${phoneprefix}" >> $LOG_FILE
	db_set oscar-emr/phoneprefix ${phoneprefix}
	echo "grep the previous db_name from the properties file" >> $LOG_FILE
	priorschema=$(sed '/^\#/d' ${PROGRAM}.tmp | grep 'db_name'  | tail -n 1 | cut -d "=" -f2- | sed 's/^[[:space:]]*//;s/[[:space:]]*$//') 2>>$LOG_FILE
	echo "priorschema=${priorschema}" >> $LOG_FILE
	db_set oscar-emr/priorschema ${priorschema} 
else
	db_set oscar-emr/upgrade false
	echo "use the supplied default properties file" >>$LOG_FILE
	echo "adjust the paths from a dev build to a deb build" >>$LOG_FILE
	echo "and comment out all of the properties that will be set below" >>$LOG_FILE
	#-e 's/\(^backup_path\)/#\1/' \  ## the backup moves to the new spot
	sed \
	-e 's/\/usr\/local\/tomcat/\/var\/lib\/${TOMCAT}/' \
	-e 's/\(^project_home\)/#\1/' \
	-e 's/\(^tomcat_path\)/#\1/' \
	-e 's/\(^login_local_ip\)/#\1/' \
	-e 's/\(^buildDateTime\)/#\1/' \
	-e 's/\(^buildtag\)/#\1/' \
	-e 's/\(^drugref_url\)/#\1/' \
	-e 's/\(^db_name =\)/#\1/' \
	-e 's/\(^db_password =\)/#\1/' \
	-e 's/\(^hibernate.dialect\)/#\1/' \
	-e 's/\(^billregion=\)/#\1/' \
	-e 's/\(^phoneprefix =\)/#\1/' \
	-e 's/\(^billcenter =\)/#\1/' \
	-e 's/\(^SINGLE_PAGE_CHART\)/#\1/' \
	-e 's/\(^signature_tablet\)/#\1/' \
	-e 's/\(^appt_intake_form\)/#\1/' \
	-e 's/\(^new_flowsheet_enabled\)/#\1/' \
	-e 's/\(^enableFax\)/#\1/' \
	-e 's/\(^DOCUMENT_DIR\)/#\1/' \
	-e 's/\(^HL7_COMPLETED_DIR\)/#\1/' \
	-e 's/\(^ENABLE_EDIT_APPT_STATUS\)/#\1/' \
	-e 's/\(^rx_enhance\)/#\1/' \
	-e 's/\(^rx_signature_enabled\)/#\1/' \
	-e 's/\(^consultation_signature_enabled\)/#\1/' \
	-e 's/\(^moh_file_management_enabled\)/#\1/' \
	-e 's/\(^ONEDT_INBOX\)/#\1/' \
	-e 's/\(^ONEDT_OUTBOX\)/#\1/' \
	-e 's/\(^ONEDT_SENT\)/#\1/' \
	-e 's/\(^ONEDT_ARCHIVE\)/#\1/' \
	-e 's/\(^workflow_enhance\)/#\1/' \
	-e 's/\(^indivica_hc_read_enable\)/#\1/' \
	-e 's/\(^eform_fax_enabled\)/#\1/' \
	-e 's/\(^eform_signature_enabled\)/#\1/' \
	-e 's/\(^eform_generator_indivica_fax_enabled\)/#\1/' \
	-e 's/\(^eform_generator_indivica_signature_enabled\)/#\1/' \
	-e 's/\(^eform_generator_indivica_print_enabled\)/#\1/' \
	-e 's/\(^indivica_rich_text_letter_enabled\)/#\1/' \
	-e 's/\(^tickler_warn_period\)/#\1/' \
	-e 's/\(^seconds_till_considered_stale\)/#\1/' \
	-e 's/\(^enable_wait_list_email_notifications\)/#\1/' \
	-e 's/\(^wait_list_email_notification_period\)/#\1/' \
	-e 's/\(^wait_list_email_notification_program_ids\)/#\1/' \
	-e 's/\(^tickler_edit_enabled\)/#\1/' \
	-e 's/\(^tickler_email_enabled\)/#\1/' \
	-e 's/\(^tickler_email_from_address\)/#\1/' \
	-e 's/\(^tickler_email_subject\)/#\1/' \
	-e 's/\(^faxEnable\)/#\1/' \
	-e 's/\(^rx_fax_enabled\)/#\1/' \
	-e 's/\(^consultation_fax_enabled\)/#\1/' \
	-e 's/\(^consultation_indivica_attachment_enabled\)/#\1/' \
	-e 's/\(^consultation_dynamic_labelling_enabled\)/#\1/' \
	-e 's/\(^WKHTMLTOPDF_COMMAND\)/#\1/' \
	-e 's/\(^oscar_port\)/#\1/' \
	-e 's/\(^logintitle\)/#\1/' \
	-e 's/\(^logintext\)/#\1/' \
	-e 's/\(^loginlogo\)/#\1/' \
	-e 's/\(^onare_labreqver\)/#\1/' \
	-e 's/\(^health_tracker\)/#\1/' \
	-e 's/\(^myOSCAR.url\)/#\1/' \
	-e 's/\(^myoscar_server_base_url\)/#\1/' \
	-e 's/\(^HL7TEXT_LABS\)/#\1/' \
	-e 's:usr/local/OscarDocument:usr/share/oscar\-emr/OscarDocument:' \
	-e 's:usr/local/tomcat/OscarDocument:usr/share/oscar\-emr/OscarDocument:' \
	-e 's:usr/local/tomcat/webapps/OscarDocument:usr/share/oscar\-emr/OscarDocument:' \
	-e 's:var/lib/OscarDocument:usr/share/oscar\-emr/OscarDocument:' \
	-e 's:var/lib/tomcat6/webapps/OscarDocument:usr/share/oscar\-emr/OscarDocument:' \
	-e 's/oscar_mcmaster/oscar/' \
	${SRC}oscar_mcmaster.properties > ${PROGRAM}.tmp 2>>$LOG_FILE
	echo "">> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo "## ------------------------------------------------------------------------------- ##">> ${PROGRAM}.tmp 2>>$LOG_FILE	
	echo "## | The following values generated by ${PROGRAM}.deb will override any set above | ##">> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo "## ------------------------------------------------------------------------------- ##">> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo "">> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo logintext=${logintext}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo loginlogo=${loginlogo}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo drugref_url=${drugref_url}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo project_home=${PROGRAM}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo tomcat_path=${tomcat_path}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo backup_path=${backup_path}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo billcenter=${billcenter}>> ${PROGRAM}.tmp 2>>$LOG_FILE

	# probe for the local ip and set it
	#login_local_ip=`ip -f inet addr show dev eth1 | sed -n 's/^ *inet *\([.0-9/]*\)\..*/\1/p'`;
    login_local_ip=`ifconfig  | grep 'inet addr:'| grep -v '127.0.0.1' | cut -d: -f2 | awk '{ print $1}'`;
	echo login_local_ip=${login_local_ip}>> ${PROGRAM}.tmp 2>>$LOG_FILE

	echo hibernate.dialect=org.oscarehr.util.persistence.OscarMySQL5Dialect>> ${PROGRAM}.tmp 2>>$LOG_FILE
	# The following directories are set to their defaults by BASE_DOCUMENT_DIR
	# If this property is set it replaces all of the other properties with absolute file paths to the OscarDocument directory.
	# The needed directories will be created from this directory
	# ie. for BASE_DOCUMENT_DIR=/usr/share/oscar-emr/OscarDocument
	#
	# DOCUMENT_DIR will be create as /usr/share/oscar-emr/OscarDocument/<Context>/document
	# with context for oscar.war being oscar

	# "HOME_DIR", "/billing/download/"
	# "DOCUMENT_DIR", "/document/" 
	# "eform_image", "/eform/images/" 
	# "oscarMeasurement_css_upload_path", "/oscarEncounter/oscarMeasurements/styles/"
	# "TMP_DIR", "/export/"
	# "form_record_path", "/form/records/"
	# "OMD_hrm","/hrm/"
	# "OMD_directory" , "/hrm/OMD/"
	# "OMD_log_directory" , "/hrm/logs/"
	# "OMD_stored", "/hrm/stored/"
	# "OMD_downloads","/hrm/sftp_downloads/"

	echo BASE_DOCUMENT_DIR=${BASE_DOCUMENT_DIR}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	#echo "eform_image=${SRC}OscarDocument/${PROGRAM}/eform/images/">> ${PROGRAM}.tmp 2>>$LOG_FILE
	#echo "TMP_DIR: ${SRC}OscarDocument/${PROGRAM}/export/">> ${PROGRAM}.tmp 2>>$LOG_FILE
	chown -R ${TOMCAT}:${TOMCAT} ${SRC}OscarDocument

fi

echo "add the required changes to the property file" >>$LOG_FILE
echo "# Creating  Custom ${PROGRAM} Property file" >> $LOG_FILE

echo buildDateTime=$buildDateTime>> ${PROGRAM}.tmp 2>>$LOG_FILE
echo buildtag="${VERSION}.${REVISION}">> ${PROGRAM}.tmp 2>>$LOG_FILE
echo debInstall=true >> ${PROGRAM}.tmp 2>>$LOG_FILE
	
echo logintitle=${logintitle}>> ${PROGRAM}.tmp 2>>$LOG_FILE

#processor=$(uname -m)
#if [ "$processor" = "x86_64" ]; then
#	echo "linking in 64 bit wkhtmltopdf" >>$LOG_FILE
#	echo WKHTMLTOPDF_COMMAND=${SRC}OscarDocument/wkhtmltopdf>> ${PROGRAM}.tmp 2>>$LOG_FILE
#else
#	echo WKHTMLTOPDF_COMMAND=${SRC}OscarDocument/wkhtmltopdf-i386>> ${PROGRAM}.tmp 2>>$LOG_FILE
#fi
#echo WKHTMLTOPDF_COMMAND=/usr/local/bin/OscarDocument/wkhtmltopdf>> ${PROGRAM}.tmp 2>>$LOG_FILE
echo WKHTMLTOPDF_COMMAND=wkhtmltopdf>> ${PROGRAM}.tmp 2>>$LOG_FILE
# Letter page size, margins as indicated, use print media stylesheet
echo WKHTMLTOPDF_ARGS=-s Letter -T 10mm -L 8mm -R 8mm --print-media-type>> ${PROGRAM}.tmp 2>>$LOG_FILE
# This deb will install a server.xml that directs Tomcat 7 for port 8443 for https connections
echo oscar_port=8443>> ${PROGRAM}.tmp 2>>$LOG_FILE

echo priorschema=${priorschema}>> ${PROGRAM}.tmp 2>>$LOG_FILE
echo db_name=${db_name}${db_switch}>> ${PROGRAM}.tmp 2>>$LOG_FILE
echo db_password=${db_password}>> ${PROGRAM}.tmp 2>>$LOG_FILE
echo billregion=${billregion}>> ${PROGRAM}.tmp 2>>$LOG_FILE
echo phoneprefix=${phoneprefix}>> ${PROGRAM}.tmp 2>>$LOG_FILE


if [ "${skip12}" = "false" ] ; then
	echo "properties introduced for 12 " >>$LOG_FILE
	# skip if upgrading from 12 
	# as you have already copied them to ${PROGRAM}.tmp
	echo "">> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo "## The following are new for Oscar 12">> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo signature_tablet=${signature_tablet}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo appt_intake_form=${appt_intake_form}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo new_flowsheet_enabled=${new_flowsheet_enabled}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo enableFax=${enableFax}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	#echo DOCUMENT_DIR=${DOCUMENT_DIR}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo HL7_COMPLETED_DIR=${HL7_COMPLETED_DIR}>> ${PROGRAM}.tmp 2>>$LOG_FILE
	echo ENABLE_EDIT_APPT_STATUS=${ENABLE_EDIT_APPT_STATUS}>> ${PROGRAM}.tmp 2>>$LOG_FILE
fi

if [ "${skip121}" = "false" ] ; then
	echo "properties introduced for 12.1"  >>$LOG_FILE
